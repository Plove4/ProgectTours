<Window x:Class="ProgectTours.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgectTours"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000" MinHeight="600" MinWidth="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="65"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#bee3e8">
            <Image Source="Resources/TOUR.png" HorizontalAlignment="Left" VerticalAlignment="Center" Height="30" Margin="10 0 0 0"></Image>
        </Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame Grid.Row="1" Source="Pages/TourPage.xaml"></Frame> 
    </Grid>
</Window>
